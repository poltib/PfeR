<ul class="none">
	<li class="fileExt"><%= @fileExt %></li>
	<% for coord in @coords %>
		<% for latlang in coord %>
			<li class="<%= latlang[0] %>"><%= latlang[1] %></li>
		<% end %>
	<% end %>
</ul>

<section>
  <div class="container">
    <h2 role="heading" aria-level="2" class="hidden">Content</h2>
    <h2 class="raceTitle"><span>Liège | <%= @happening.date %></span><%= @happening.name %></h2>
    <div class="head">
      <div class="visuels">
        <div class="map">
					<div id="happening-map"></div>
        	<div id="elevation_chart"></div>
        </div>
        <div class="aside">
          <div class="users">
            <h3 role="heading" aria-level="3">Participer</h3>
            <p>123 utilisateurs y participent</p>
          </div>
          <ul class="actions">
            <li><a href="#"><i class="icon-check"></i>Participer</a></li>
            <li><a href="#"><i class="icon-user-add"></i>Inviter amis</a></li>
            <li><a href="#">&#9829; (89)</a></li>
            <li class="shareButton"><i class="shareDart">&rarr;</i>&nbsp;share
              <ul class="shareForum">
                <li><a href="#"><i class="icon-facebook-circled"></i></a></li>
                <li><a href="#"><i class="icon-gplus-circled"></i></a></li>
                <li><a href="#"><i class="icon-twitter-circled"></i></a></li>
                <li><a href="#"><i class="icon-mail"></i></a></li>
                <li><a href="#"><i class="icon-rss"></i></a></li>
              </ul>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <div class="raceDetails">
      <div class="informations">
        <h3 role="heading" aria-level="3">Informations</h3>
        <ul>
          <li class="address">Rue Haute 11 400 flémalle</li>
          <li>description :</li>
        </ul>
      </div>
      <div class="gallery">
        <h3 role="heading" aria-level="3">Gallerie photo</h3>
        <ul class="thumbs">
          <li><img src="../img/race.png"></li>
        </ul>
      </div>
    </div>
    <div class="sponsors">
      <h3 role="heading" aria-level="3">Sponsors</h3>
      <ul class="thumbs">
        <li><img src="../img/race.png"></li>
      </ul>
    </div>
  </div>
</section>
<% content_for :script do %>
	<%= javascript_include_tag "happening" %>
<% end %>